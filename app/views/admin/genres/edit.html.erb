<div class="container">
<h4 class="row mt-4">
ジャンル編集
</h4>
</div>
<div class="row">
 <div class="mx-auto">
    <%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch do |f| %>
    <label class="col-form-label">ジャンル名</label>
    <div class="col-sm-2">
    <%= f.text_field :name %>
   <%= f.submit "変更を保存", class: "btn btn-success" %>
 </div>
</div>
</div>
 <% end %>