<div class="container">
<h4 class="row mt-4 bg-light">
ジャンル編集
</h4>

<%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch do |f| %>
<table class="table table-borderless">
    <tr>
      <th>ジャンル名 <%= f.text_field :name %><%= f.submit "変更を保存", class: "btn btn-success" %></th>
    </tr>
</table>
<% end %>